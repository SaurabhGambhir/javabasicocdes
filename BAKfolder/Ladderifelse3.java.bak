class Ladderifelse3 
{
	public static void main(String[] args) 
	{
		int no=-13;
		if(no%2==0)//even
		{
			if(no>0)//positive
			{System.out.println("positive even");}
			else if (no<0)//negative
			{System.out.println("negative even");}
			else //zero
			{System.out.println("zero");}
			
		
		}
		
		
		
		else//odd
		{
			if(no>0)//positive
			{System.out.println("positive even");}
			else if (no<0)//negative
			{System.out.println("negative even");}
			
		}
	}
}
